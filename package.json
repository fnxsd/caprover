{
  "name": "one-click-apps",
  "version": "1.1.2",
  "description": "One Click App Repository for CapRover",
  "scripts": {
    "inst": "ncu -u && typesync && pnpm i && pettier -w ./package.json",
    "up": "npm run inst",
    "format:check": "prettier --check \"./public/**/*.(json|yml)\"",
    "format:public": "prettier --write \"./public/**/*.(json|yml)\"",
    "format:workflows": "prettier --write \"./.github/workflows/**/*.yml\"",
    "format:scripts": "prettier --write \"./scripts/**.js\"",
    "format": "run-p format:public format:workflows format:scripts",
    "clean": "rimraf ./dist/ && mkdirp dist && echo 'clean'",
    "build_one_click_apps": "node ./scripts/build_one_click_apps.js",
    "build_one_click_apps_v4": "node ./scripts/build_one_click_apps_from_v4.js",
    "build": "run-s clean build_one_click_apps  build_one_click_apps_v4",
    "validate_apps": "node ./scripts/validate_apps.js",
    "validate-apps": "node ./scripts/validate-apps.js",
    "publish:action": "./scripts/publish-from-actions.sh",
    "publish": "run-s build publish:action",
    "gh-pages": "run-s clean format build && node ./scripts/gh-pages.js",
    "new": "node  ./scripts/new-app.js",
    "new-app": "npm run new",
    "n": "npm run new",
    "rename": "node ./scripts/rename-app.js",
    "mv": "npm run rename",
    "rm": "node ./scripts/remove-app.js",
    "del": "npm run rm",
    "cli:build": "shx rm -rf app && tsc -b",
    "cli:lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "cli:test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "cli:version": "oclif readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fnxsd/caprover.git"
  },
  "oclif": {
    "bin": "ch",
    "dirname": "ch",
    "commands": "./app/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "main": "app/index.js",
  "files": [
    "/bin",
    "/app",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "author": "hharzer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fnxsd/caprover/issues"
  },
  "homepage": "https://github.com/fnxsd/caprover",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "1.3.0",
    "@oclif/plugin-commands": "2.2.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "chalk": "^5.0.1",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "mkdirp": "1.0.4",
    "oclif": "^3.0.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "semver": "^7.3.5",
    "typesync": "0.8.0",
    "yaml": "^1.10.2",
    "yargs": "17.5.1"
  },
  "bin": {
    "ch": "./bin/run"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/eslint": "^8.4.3",
    "@types/fs-extra": "^9.0.13",
    "@types/gh-pages": "^3.2.0",
    "@types/mkdirp": "1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/prettier": "^2.4.3",
    "@types/rimraf": "3.0.2",
    "@types/semver": "^7.3.9",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "17.0.10",
    "caprover": "2.2.3",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "madrun": "8.10.1",
    "mocha": "^9",
    "npm-check-updates": "12.2.1",
    "npm-run-all": "4.1.5",
    "oclif": "^3",
    "prettier": "^2.5.1",
    "redrun": "9.0.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1"
  },
  "pnpm": {
    "enable-pre-post-scripts": true
  },
  "np": {
    "yarn": false,
    "contents": "dist",
    "tests": false,
    "publish": false
  },
  "types": "app/index.d.ts"
}
